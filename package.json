{
    "name": "node-raphael",
    "description": "wrapper for raphael for usage in nodejs, compatible with raphael 2.1.0",
    "version": "0.1.8",
    "homepage": "https://github.com/dodo/node-raphael",
    "repository": {
        "type": "git",
        "url": "git://github.com/kurmbox/node-raphael.git"
    },
    "author": "dodo (https://github.com/dodo)",
